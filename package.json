{
  "name": "a11yscan-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:public": "cd public-website && npm run dev",
    "dev:both": "npm-run-all --parallel dev dev:public",
    "build": "next build 2>&1 | tee build.log",
    "build:main": "next build",
    "build:public": "cd public-website && npm run build",
    "export:main": "next build",
    "export:public": "cd public-website && npm run export",
    "start": "next start",
    "dev:emulators": "firebase emulators:start --only firestore,storage,functions,auth --import=./emulator-data --export-on-exit=./emulator-data",
    "deploy:main": "npm run export:main && firebase deploy --only hosting:main-app",
    "deploy:public": "npm run export:public && firebase deploy --only hosting:public-website",
    "deploy:backend": "firebase deploy --only functions,firestore:rules,storage:rules",
    "deploy:all": "npm run export:main && npm run export:public && firebase deploy"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.0",
    "axe-core": "^4.10.3",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.9",
    "firebase": "^9.22.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.5.0",
    "next": "16.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5"
  }
}
